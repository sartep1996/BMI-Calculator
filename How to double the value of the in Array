package com.petras;

import java.util.Arrays;

public class Main {
    public static void main(String[] arg) {
        int[] inputArray = {1, 2, 3};
        for (int i = 0; i < inputArray.length; i++) {
            inputArray[i] *= 2;
        }
            System.out.println(Arrays.toString(inputArray));
        }
    }
